            // "muutuja"    -> –∏–º—è –ø–µ—Ä–µ–º–µ–Ω–Ω–æ–π
            // =            -> –æ–ø–µ—Ä–∞—Ç–æ—Ä –ø—Ä–∏—Å–≤–∞–∏–≤–∞–Ω–∏—è
            // 3            -> –ø—Ä–∏—Å–≤–∞–∏–≤–∞–µ–º–æ–µ –∑–Ω–∞—á–µ–Ω–∏–µ

            /* –í–æ–∑–º–æ–∂–Ω—ã–µ —Ç–∏–ø—ã –¥–∞–Ω–Ω—ã—Ö */
            int a = 1;                // —Ü–µ–ª–æ–µ —á–∏—Å–ª–æ
            decimal b = 2.1M;         // –¥–µ—Å—è—Ç–∏—á–Ω–æ–µ —á–∏—Å–ª–æ —Å –ø–ª–∞–≤–∞—é—â–µ–π —Ç–æ—á–∫–æ–π
            float c = 3.9f;           // 32-–±–∏—Ç–Ω–æ–µ —á–∏—Å–ª–æ —Å –ø–ª–∞–≤–∞—é—â–µ–π —Ç–æ—á–∫–æ–π
            double d = 5.6d;          // 64-–±–∏—Ç–Ω–æ–µ —á–∏—Å–ª–æ —Å –ø–ª–∞–≤–∞—é—â–µ–π —Ç–æ—á–∫–æ–π
            char c1 = 'a';            // –æ–¥–∏–Ω —Å–∏–º–≤–æ–ª
            string s = "—Ç–µ–∫—Å—Ç";       // —Å—Ç—Ä–æ–∫–∞
            var x = "abc";            // –Ω–µ—è–≤–Ω—ã–π —Ç–∏–ø
            var y = 123;
            const int z = 9;          // –∫–æ–Ω—Å—Ç–∞–Ω—Ç–∞ (–Ω–µ–ª—å–∑—è –∏–∑–º–µ–Ω–∏—Ç—å)
            // void ‚Äî —Ç–∏–ø –¥–∞–Ω–Ω—ã—Ö, –∫–æ—Ç–æ—Ä—ã–π –Ω–µ–ª—å–∑—è –∏—Å–ø–æ–ª—å–∑–æ–≤–∞—Ç—å –¥–ª—è –ø–µ—Ä–µ–º–µ–Ω–Ω—ã—Ö, —Ç–æ–ª—å–∫–æ –¥–ª—è –º–µ—Ç–æ–¥–æ–≤

            /* –í–æ–∑–º–æ–∂–Ω—ã–µ —Å–æ—Å—Ç–∞–≤–Ω—ã–µ —Ç–∏–ø—ã –¥–∞–Ω–Ω—ã—Ö */

            // 1. –ú–∞—Å—Å–∏–≤:
            // [] ‚Äî –º–∞—Å—Å–∏–≤ –º–æ–∂–µ—Ç —Ö—Ä–∞–Ω–∏—Ç—å –Ω–µ—Å–∫–æ–ª—å–∫–æ —ç–ª–µ–º–µ–Ω—Ç–æ–≤ –æ–¥–Ω–æ–≥–æ —Ç–∏–ø–∞
            // –º–∞—Å—Å–∏–≤ –∏–º–µ–µ—Ç —Ñ–∏–∫—Å–∏—Ä–æ–≤–∞–Ω–Ω—É—é –¥–ª–∏–Ω—É
            int[] arvuMassiiv = new int[3];

            // –í—Ç–æ—Ä–æ–π —Å–ø–æ—Å–æ–± —Å–æ–∑–¥–∞–Ω–∏—è –º–∞—Å—Å–∏–≤–∞:
            int[] arvuMassiiv2 = { 1, 2, 3 };

            // –í—Å—Ç—Ä–æ–µ–Ω–Ω—ã–µ –º–µ—Ç–æ–¥—ã –º–∞—Å—Å–∏–≤–∞:
            int hasThisMany = arvuMassiiv.Length;

            // 2. –°–ø–∏—Å–æ–∫ (List):
            // List<T> ‚Äî –¥–∏–Ω–∞–º–∏—á–µ—Å–∫–∏–π —Å–ø–∏—Å–æ–∫
            List<int> arvuNimekiri = new List<int>();

            // –í—Ç–æ—Ä–æ–π —Å–ø–æ—Å–æ–± —Å–æ–∑–¥–∞–Ω–∏—è —Å–ø–∏—Å–∫–∞:
            List<int> arvuNimekiri2 = new List<int>() { 1, 2, 3 };

            // –¢—Ä–µ—Ç–∏–π —Å–ø–æ—Å–æ–±:
            List<int> arvuNimekiri3 = new List<int>(3);

            int aa = 9001;

            // –ú–µ—Ç–æ–¥—ã —Å–ø–∏—Å–∫–∞:
            arvuNimekiri.Add(99);
            arvuNimekiri.Add(aa);

            int loendiPikkus = arvuNimekiri3.Count();
            bool KasSeeArvOn = arvuNimekiri3.Contains(3);
            arvuNimekiri3.Remove(4);

            // –û—Å–Ω–æ–≤–Ω—ã–µ –º–∞—Ç–µ–º–∞—Ç–∏—á–µ—Å–∫–∏–µ –æ–ø–µ—Ä–∞—Ü–∏–∏
            int liitmine = 1 + 1;
            int lahutamine = 1 - 1;
            double korrutamine = 1 * 2;
            double jagamine = 1 / 2;

            // –ú–∞—Ç–µ–º–∞—Ç–∏—á–µ—Å–∫–∏–µ –æ–ø–µ—Ä–∞—Ü–∏–∏ –∏–∑ –º–æ–¥—É–ª—è Math
            double astendamine = Math.Pow(2, 2);
            double juurimine = Math.Sqrt(2);
            double newD = 4.0000001d;

            Math.Round(newD);
        }
    }
}
